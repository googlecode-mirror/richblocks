	<function name="showTitleApplication">
		<![CDATA[
			if(RB_BAR_APPLICATION_DISPLAY){
				document.getElementById('frame_bar_top').style.display = 'none';
				document.getElementById('frame_menu').style.top = '0px';
				document.getElementById('frame_buttons').style.top = '22px';
				RB_BAR_APPLICATION_DISPLAY = false;
			}else{
				document.getElementById('frame_bar_top').style.display = 'block';
				document.getElementById('frame_menu').style.top = '22px';
				document.getElementById('frame_buttons').style.top = '43px';
				RB_BAR_APPLICATION_DISPLAY = true;
			}
			
		]]>
	</function>
	
